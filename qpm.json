{
    "sharedDir": "shared",
    "dependenciesDir": "extern",
    "version": "0.1.0",
    "info": {
        "name": "Haptic Revert",
        "id": "haptic-revert",
        "version": "0.2.0",
        "url": null,
        "additionalData": {
            "overrideSoName": "libhaptic-revert.so",
            "cmake": true
        }
    },
    "workspace": {
        "scripts": {
            "build": [
                "pwsh ./scripts/build.ps1"
            ],
            "clean": [
                "pwsh ./scripts/build.ps1 -clean"
            ],
            "copy": [
                "pwsh ./scripts/copy.ps1"
            ],
            "logcat": [
                "pwsh ./scripts/start-logging.ps1"
            ],
            "qmod": [
                "pwsh ./scripts/build.ps1",
                "pwsh ./scripts/createqmod.ps1"
            ],
            "restart": [
                "pwsh ./scripts/restart-game.ps1"
            ],
            "stack": [
                "pwsh ./scripts/ndk-stack.ps1"
            ],
            "tomb": [
                "pwsh ./scripts/pull-tombstone.ps1"
            ]
        },
        "qmodIncludeDirs": [
            "./build",
            "./extern/libs"
        ],
        "qmodOutput": "HapticRevert.qmod"
    },
    "dependencies": [
        {
            "id": "beatsaber-hook",
            "versionRange": "^5.1.9",
            "additionalData": {
                "extraFiles": [
                    "src/inline-hook"
                ]
            }
        },
        {
            "id": "scotland2",
            "versionRange": "^0.1.4",
            "additionalData": {
                "includeQmod": false
            }
        },
        {
            "id": "paper",
            "versionRange": "^3.6.3",
            "additionalData": {}
        },
        {
            "id": "bs-cordl",
            "versionRange": "^3700.0.0",
            "additionalData": {}
        }
    ]
}